{
    "name": "wizard-generator",
    "version": "1.0.0",
    "description": "This is an cli tool which lets you create your onw template and generate files based on it.",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "dev": "node index.js",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles --verbose",
        "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/unit --detectOpenHandles --verbose",
        "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/integration --detectOpenHandles --verbose",
        "test:e2e": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/e2e --detectOpenHandles --verbose",
        "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --detectOpenHandles --verbose",
        "test:basic": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/basic.test.js --verbose"
    },
    "keywords": [
        "cli",
        "generator",
        "scaffold",
        "templates",
        "boilerplate-generator"
    ],
    "author": "Vivek Prajapati",
    "license": "ISC",
    "dependencies": {
        "inquirer": "^8.0.0"
    },
    "devDependencies": {
        "@jest/globals": "^29.7.0",
        "jest": "^29.7.0"
    },
    "bin": {
        "wizard": "./index.js"
    }
}
